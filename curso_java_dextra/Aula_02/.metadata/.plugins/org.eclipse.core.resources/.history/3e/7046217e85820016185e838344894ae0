package Aula02;

public class Ponto {
	
	// Inicializando com valor fixo.
	double x = 0.0;
	// Inicializado com valor padr√£o.
	double y;
	
	
	/**
	 * Metodo descolar que recebe dois parametros double.
	 * @param deltax
	 * @param deltay
	 */
	
	public void deslocar(double deltax, double deltay) {
		x += deltax;
		y += deltay;
		
	}

}
